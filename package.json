    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.3",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "remix-i18next": "^5.5.0",
    "tailwind-merge": "^2.2.1"
